<h1>Liste der Abstimmungen</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Cdu party</th>
    <th>Spd party</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @votes.each do |vote| %>
  <tr>
    <td><%= vote.id %></td>
    <td><%= vote.cdu_party %></td>
    <td><%= vote.spd_party %></td>
    <td><%= link_to 'Show', vote %></td>
    <td><%= link_to 'Edit', edit_vote_path(vote), :class => "btn btn-mini" %></td>
    <td><%= link_to 'Destroy', vote, method: :delete, data: { confirm: 'Are you sure?' }, :class => "btn btn-mini btn-danger" %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Vote', new_vote_path %>
